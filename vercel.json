{
  "version": 2,
  "framework": "static",
  "buildCommand": false,
  "installCommand": false,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/ftd-pc/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
